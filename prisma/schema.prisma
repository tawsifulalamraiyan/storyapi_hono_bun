
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model storydb{
  id Int @id @default(autoincrement())
  title String
  writter String
  content String
  createdAt DateTime @default(now())
}